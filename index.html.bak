  <script>
    document.addEventListener('DOMContentLoaded', function () {
      // Testimonials carousel
      const carousel = document.querySelector('.testimonial-carousel');
      if (carousel) {
        const track = carousel.querySelector('.testimonial-track');
        const items = Array.from(carousel.querySelectorAll('.testimonial-item'));
        const dotsContainer = carousel.querySelector('.testimonial-dots');
        let itemsPerView = 1;
        let pages = 1;
        let index = 0;
        let timer;

        function getItemsPerView() {
          const w = window.innerWidth;
          if (w >= 1200) return 4; // super wide
          if (w >= 768) return 3;  // desktop
          if (w >= 480) return 2;  // tablet
          return 1;                // mobile
        }

        function buildDots() {
          dotsContainer.innerHTML = '';
          for (let i = 0; i < pages; i++) {
            const btn = document.createElement('button');
            btn.className = 'testimonial-dot';
            btn.setAttribute('aria-label', 'Slide ' + (i + 1));
            btn.addEventListener('click', () => slideTo(i));
            dotsContainer.appendChild(btn);
          }
        }

        function slideTo(i) {
          index = (i + pages) % pages;
          track.style.transform = 'translateX(' + (-index * 100) + '%)';
          const dots = Array.from(dotsContainer.querySelectorAll('.testimonial-dot'));
          dots.forEach((d, di) => d.classList.toggle('active', di === index));
        }

        function startTimer() {
          clearInterval(timer);
          timer = setInterval(() => slideTo(index + 1), 5000);
        }

        function setup() {
          itemsPerView = getItemsPerView();
          carousel.style.setProperty('--items-per-view', itemsPerView);
          pages = Math.ceil(items.length / itemsPerView);
          if (index >= pages) index = 0;
          buildDots();
          slideTo(index);
          startTimer();
        }

        setup();
        carousel.addEventListener('mouseenter', () => { clearInterval(timer); });
        carousel.addEventListener('mouseleave', () => { startTimer(); });

        let resizeTimer;
        window.addEventListener('resize', () => {
          clearTimeout(resizeTimer);
          resizeTimer = setTimeout(setup, 150);
        });
      }

      // Podcast auto-update (optional; requires channelId and CORS).
      const podcastGrid = document.querySelector('.yt-grid');
      const channelId = podcastGrid ? podcastGrid.dataset.channelId : '';
      if (podcastGrid && channelId) {
        const rssUrl = `https://www.youtube.com/feeds/videos.xml?channel_id=${channelId}`;
        const proxy = 'https://api.rss2json.com/v1/api.json?rss_url=' + encodeURIComponent(rssUrl);

        fetch(proxy)
          .then(res => { if (!res.ok) throw new Error('RSS fetch failed'); return res.json(); })
          .then(data => {
            if (!data.items) throw new Error('No items');
            podcastGrid.innerHTML = '';
            data.items.slice(0, 8).forEach(item => {
              const videoId = item.guid.replace('yt:video:', '');
              const duration = item.duration ? new Date(item.duration * 1000).toISOString().substr(11, 8).replace(/^00:/, '') : '';
              const card = document.createElement('a');
              card.className = 'yt-card';
              card.href = item.link;
              card.target = '_blank';
              card.rel = 'noreferrer';
              const desc = item.description ? item.description.replace(/<[^>]+>/g, '').slice(0, 140) + '…' : '';
              card.innerHTML = `
                <div class="yt-thumb" style="background-image:url('https://img.youtube.com/vi/${videoId}/hqdefault.jpg');">
                  <div class="yt-play"><i class="bi bi-play-circle"></i></div>
                  <div class="yt-duration">${duration || ''}</div>
                </div>
                <div class="yt-body">
                  <h3 class="yt-title">${item.title}</h3>
                  <p class="yt-desc">${desc}</p>
                  <div class="yt-meta">
                    <span class="pill">Podcast</span>
                    <span>${new Date(item.pubDate).toLocaleDateString('pt-BR')}</span>
                  </div>
                </div>`;
              podcastGrid.appendChild(card);
            });
          })
          .catch(err => console.warn('Fallback to static podcast cards:', err));
      }
    });
  </script>
